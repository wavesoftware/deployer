#!/usr/bin/env bash
#

INSTALLPATH=/usr/local/lib/deployproj
EXC=$(readlink -f $0)
ROOT=$(dirname $(dirname $EXC))

echo $EXC | grep -q $INSTALLPATH
ret=$?
if test "$ret" = "0"; then
	conf=/etc/deployproj.conf
	path=$INSTALLPATH/src
else
	conf=$ROOT/etc/deployproj.conf
	path=$ROOT/src
fi
PYTHON=$(command -v python)
$PYTHON $path/deployproj.py $conf $@
ret=$?
exit $ret